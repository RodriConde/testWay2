{
  "scripts": {
    "runFrontBack": "npx cypress run",
    "runFronte2e": "npx cypress run --spec 'cypress/e2e/feature/way2automation.feature'",
    "runBack2e": "npx cypress run --spec 'cypress/e2e/feature/jsonplaceholder.feature'",
    "generateReport": "node cucumber-html-report.js",
    "combine:reports": "jrm cypress/results/combined-report.xml \"cypress/results/*.xml\"",
    "postreport": "npm run combine:reports",
    "report:merge": "mochawesome-merge cypress/results/*.json -o cypress/report.json",
    "report:generate": "marge cypress/report.json --charts true",
    "html-generator": " npx mochawesome-report-generator mochawesome.json"
  },
  "devDependencies": {
    "@cypress/grep": "^4.0.1",
    "cypress": "^13.4.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-split": "^1.23.0"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/report/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
